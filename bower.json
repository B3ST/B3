{
  "name": "B3",
  "description": "Backbone Bootstrap Boilerplate: A JavaScript Starter Theme for WordPress",
  "version": "0.0.1",
  "private": true,
  "homepage": "http://log.pt/",
  "authors": [
    "Luis Rodrigues <luis.rodrigues@log.pt>",
    "Pedro Sousa <pedro.cruz@log.pt>"
  ],
  "dependencies": {
    "backbone": "~1.1.2",
    "Backbone.validateAll": "*",
    "bootstrap": "~3.2.0",
    "dustjs-linkedin": "~2.5.1",
    "dustjs-linkedin-helpers": "~1.5.0",
    "dustjs-linkedin-filters-secure": "*",
    "jquery": "~2.1.0",
    "jqueryui": "~1.11.0",
    "lodash": "~2.4.1",
    "marionette": "~2.3.0",
    "requirejs": "~2.1.14",
    "text": "~2.0.12",
    "marionette-dust": "~0.1.6",
    "i18n": "requirejs/i18n#~2.0.4",
    "bootstrap-notify": "~0.1.0",
    "momentjs": "~2.8.1"
  },
  "devDependencies": {
    "jasmine": "~2.0.3",
    "jasmine-jquery": "~2.0.5",
    "sinon": "~1.10.3",
    "socket.io": "~1.0.6"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test"
  ],
  "overrides": {
    "Backbone.validateAll": {
      "main": "src/javascripts/Backbone.validateAll.min.js"
    },
    "bootstrap-notify": {
      "main": [
        "js/bootstrap-notify.js",
        "css/bootstrap-notify.css"
      ]
    },
    "browser-sync-client": {
      "main": "dist/index.min.js"
    },
    "dustjs-linkedin-helpers": {
      "main": "dist/dust-helpers.min.js"
    },
    "dustjs-linkedin-filters-secure": {
      "main": "lib/dust-filters-secure.js"
    },
    "text": {
      "main": "text.js"
    },
    "i18n": {
      "main": "i18n.js"
    },
    "sinon": {
      "main": "pkg/sinon.js"
    },
    "jasmine": {
      "main": [
        "lib/jasmine-core/jasmine.js",
        "lib/jasmine-core/jasmine-html.js",
        "lib/jasmine-core/jasmine.css",
        "lib/jasmine-core/boot/boot.js",
        "lib/jasmine-core/json2.js"
      ]
    },
    "socket.io": {
      "main": [
        "lib/index.js",
        "lib/socket.js",
        "lib/namespace.js",
        "lib/client.js"
      ]
    }
  }
}
